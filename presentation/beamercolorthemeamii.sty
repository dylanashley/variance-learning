\ProvidesPackage{beamercolorthemeamii}

\definecolor{black}{RGB}{0, 0, 0}
\definecolor{blue}{RGB}{22, 87, 136}
\definecolor{gold}{RGB}{212, 186, 0}
\definecolor{maroon}{RGB}{100, 31, 69}
\definecolor{navy}{RGB}{0, 66, 80}
\definecolor{olive}{RGB}{199, 210, 138}
\definecolor{navy-olive}{RGB}{100, 138, 109}

\setbeamercolor{title}{fg=navy}
\setbeamercolor{background canvas}{bg=white}
\setbeamercolor{normal text}{fg=black}
\setbeamercolor{alerted text}{fg=gold!150}
\setbeamercolor{structure}{fg=navy}
\setbeamercolor{palette primary}{fg=navy}
\setbeamercolor{palette secondary}{fg=navy}
\setbeamercolor{palette tertiary}{fg=white, bg=navy-olive}
\setbeamercolor{palette quaternary}{fg=navy!3, bg=navy}
\setbeamercolor{frametitle}{parent=palette quaternary}

\setbeamercolor{page number in head/foot}{fg=navy}

\mode
<all>
